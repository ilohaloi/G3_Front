[32m+[m[32mfunction displayCoupons(usable, used, expired) {[m
[32m+[m[32m    $("#usableCoupons, #usedCoupons, #expiredCoupons").hide(); //初始隱藏所有類別[m
[32m+[m[32m    console.log("可使用的優惠券:", usable);[m
[32m+[m[32m    console.log("已使用的優惠券:", used);[m
[32m+[m[32m    console.log("已過期的優惠券:", expired);[m
[32m+[m
[32m+[m[32m    // 顯示可使用的優惠券[m
[32m+[m[32m    if (usable.length > 0) {[m
[32m+[m[32m        $("#usableCoupons").html(usable.map(coupon => `<div>${coupon.name} - 到期日: ${coupon.expiry_date}</div>`).join(''));[m
[32m+[m[32m        $("#usableCoupons").show(); // 顯示可使用的區域[m
[32m+[m[32m    } else {[m
[32m+[m[32m        $("#usableCoupons").html('<p>目前沒有可使用的優惠券</p>');[m
[32m+[m[32m        $("#usableCoupons").show(); // 仍然顯示可使用的區域[m
[32m+[m[32m        $(".no-coupons").show(); // 顯示沒有優惠券的消息[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // 顯示已使用的優惠券[m
[32m+[m[32m    if (used.length > 0) {[m
[32m+[m[32m        $("#usedCoupons").html(used.map(coupon => `<div>${coupon.name} - 到期日: ${coupon.expiry_date}</div>`).join(''));[m
[32m+[m[32m        $("#usedCoupons").show(); // 顯示已使用的區域[m
[32m+[m[32m    } else {[m
[32m+[m[32m        $("#usedCoupons").html('<p>目前沒有已使用的優惠券</p>');[m
[32m+[m[32m        $("#usedCoupons").show(); // 仍然顯示已使用的區域[m
[32m+[m[32m        $(".no-coupons").show(); // 顯示沒有優惠券的消息[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // 顯示已過期的優惠券[m
[32m+[m[32m    if (expired.length > 0) {[m
[32m+[m[32m        $("#expiredCoupons").html(expired.map(coupon => `<div>${coupon.name} - 到期日: ${coupon.expiry_date}</div>`).join(''));[m
[32m+[m[32m        $("#expiredCoupons").show(); // 顯示已過期的區域[m
[32m+[m[32m    } else {[m
[32m+[m[32m        $("#expiredCoupons").html('<p>目前沒有已過期的優惠券</p>');[m
[32m+[m[32m        $("#expiredCoupons").show(); // 仍然顯示已過期的區域[m
[32m+[m[32m        $(".no-coupons").show(); // 顯示沒有優惠券的消息[m
[32m+[m[32m    }[m
