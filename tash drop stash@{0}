[32m+[m[32mfunction showCoupons(type) {[m
[32m+[m[32m    // 隱藏所有優惠券類別[m
[32m+[m[32m    $(".coupon-list").hide();[m
[32m+[m
[32m+[m[32m    // 顯示選擇的優惠券類別[m
[32m+[m[32m    if (type === 'usable') {[m
[32m+[m[32m        $("#usableCoupons").show();[m
[32m+[m[32m    } else if (type === 'used') {[m
[32m+[m[32m        $("#usedCoupons").show();[m
[32m+[m[32m    } else if (type === 'expired') {[m
[32m+[m[32m        $("#expiredCoupons").show();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m     // 移除所有標籤的 `active` 類別[m
[32m+[m[32m     $(".discount-tab li").removeClass("active");[m
[32m+[m
[32m+[m[32m     // 添加 `active` 類別到當前點擊的標籤[m
[32m+[m[32m     if (type === 'usable') {[m
[32m+[m[32m         $(".discount-tab li:nth-child(1)").addClass("active");[m
[32m+[m[32m     } else if (type === 'used') {[m
[32m+[m[32m         $(".discount-tab li:nth-child(2)").addClass("active");[m
[32m+[m[32m     } else if (type === 'expired') {[m
[32m+[m[32m         $(".discount-tab li:nth-child(3)").addClass("active");[m
[32m+[m[32m     }[m
[32m+[m[32m}[m
